<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgkriya"
    tools:context=".activity.plantproduct.DetailMaterialKriyaActivity">


    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPagerDetail"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="@dimen/_200sdp"
        android:layout_centerInParent="true"/>

    <ImageView
        android:id="@+id/tanamHuman"
        android:layout_width="195dp"
        android:layout_height="172dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="@dimen/_25sdp"
        android:src="@drawable/kriyahuman"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/btnPrev"
        android:layout_width="@dimen/_30sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/_25sdp"
        android:layout_toStartOf="@id/viewPagerDetail"
        android:layout_marginBottom="@dimen/_35sdp"
        android:src="@drawable/button_prevhdpi"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/btnNext"
        android:layout_width="@dimen/_30sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_marginStart="@dimen/_25sdp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@id/viewPagerDetail"
        android:src="@drawable/button_nexthdpi"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/btn_forSubmission"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_marginBottom="@dimen/_21sdp"
        android:layout_marginEnd="@dimen/_20sdp"
        android:background="@drawable/button_submisionhdpi"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        tools:ignore="ContentDescription" />


</RelativeLayout>